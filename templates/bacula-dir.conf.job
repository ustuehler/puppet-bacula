
# <%= comment %>
Job {
  Name = "<%= name %>"
<% if jobdefs != '' -%>
  JobDefs = "<%= jobdefs %>"
<% end -%>
<% if scope.lookupvar('type') != '' -%>
  Type = <%= scope.lookupvar('type') %>
<% end -%>
  Client = <%= client %>
  FileSet = "<%= fileset %>"
<% if accurate -%>
  Accurate = yes
<% end -%>
<%= content -%>
}
