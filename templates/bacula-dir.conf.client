
# Client (File service) to backup or restore
Client {
  Name = <%= name %>
  Address = <%= address %>
  FD Port = 9102
  Catalog = MyCatalog
  Password = "<%= password %>"        # password for FileDaemon
  File Retention = 30 days            # 30 days
  Job Retention = 6 months            # six months
  AutoPrune = yes                     # Prune expired Jobs/Files
}
